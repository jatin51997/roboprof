@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix voc: <file:///Users/jatin/workspace/roboprof/schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

voc:University
    a rdfs:Class ;
    rdfs:subClassOf foaf:Organization ;
    rdfs:label "University"@en .

#foaf:name
#rdfs:seeAlso

voc:Course
    a rdfs:Class ;
    rdfs:label "An academic course offered by a university."@en .

voc:CourseSubject a rdf:Property ;
    rdfs:domain voc:Course ;
    rdfs:range rdfs:Literal ;
    rdfs:label "course subject"@en ;
    rdfs:comment "The subject code of the course."@en .

voc:CourseNumber a rdf:Property ;
    rdfs:domain voc:Course ;
    rdfs:range rdfs:Literal ;
    rdfs:label "course number"@en ;
    rdfs:comment "The number code of the course."@en .

voc:CourseCredits a rdf:Property ;
    rdfs:domain voc:Course ;
    rdfs:range rdfs:Literal ;
    rdfs:label "course credits"@en ;
    rdfs:comment "The credit value of the course."@en .

voc:CourseDescription a rdf:Property ;
    rdfs:domain voc:Course ;
    rdfs:range rdfs:Literal ;
    rdfs:label "course description"@en ;
    rdfs:comment "A description of the course."@en .

voc:CourseType a rdf:Property ;
    rdfs:domain voc:Course ;
    rdfs:range rdfs:Literal ;
    rdfs:label "course description"@en ;
    rdfs:comment "A description of the course."@en .

voc:CourseOutline a rdf:Property ;
    rdfs:domain voc:Course ;
    rdfs:label "course outline"@en ;
    rdfs:comment "A link to the course outline."@en .

#foaf:name
#rdfs:seeAlso


voc:Lecture
    a rdfs:Class ;
    rdfs:label "A session within a course that provides instruction on a specific topic."@en .

voc:LectureNumber a rdf:Property ;
    rdfs:label "lecture number"@en ;
    rdfs:comment "The sequential number of the lecture."@en ;
    rdfs:domain voc:Lecture ;
    rdfs:range rdfs:Literal .

voc:LectureContent a rdf:Property ;
    rdfs:label "lecture content"@en ;
    rdfs:comment "The content of the lecture."@en ;
    rdfs:domain voc:Lecture ;
    rdfs:range rdfs:Literal .

voc:Slides a rdfs:Class ;
    rdfs:label "Slides"@en ;
    rdfs:subClassOf voc:LectureContent .

voc:Worksheets a rdfs:Class ;
    rdfs:label "Worksheets"@en ;
    rdfs:subClassOf voc:LectureContent .

voc:Readings a rdfs:Class ;
    rdfs:label "Readings"@en ;
    rdfs:subClassOf voc:LectureContent .

voc:OtherMaterial a rdfs:Class ;
    rdfs:label "Other Material"@en ;
    rdfs:subClassOf voc:LectureContent .

voc:AdditionalResources a rdfs:Class ;
    rdfs:label "Additional Resources"@en ;
    rdfs:subClassOf voc:LectureContent .
#foaf:name
#rdfs:seeAlso

voc:Topic
    a rdfs:Class ;
    rdfs:label "A subject covered within a course."@en .

voc:TopicProvenance a rdf:Property ;
    rdfs:label "topic provenance"@en ;
    rdfs:comment "The source where the topic was identified."@en ;
    rdfs:domain voc:Topic ;
    rdfs:range voc:Lecture .

#foaf:name
#rdfs:seeAlso

voc:Student
    a rdfs:Class ;
    rdfs:subClassOf foaf:Person ;
    rdfs:label "An individual enrolled in a university."@en .

voc:IDNumber a rdf:Property ;
    rdfs:label "ID number"@en ;
    rdfs:comment "The identification number of the student."@en ;
    rdfs:domain voc:Student ;
    rdfs:range rdfs:Literal .

voc:CompletedCourse a rdf:Property ;
    rdfs:label "completed course"@en ;
    rdfs:comment "A course completed by the student."@en ;
    rdfs:domain voc:Student ;
    rdfs:range rdfs:Course .

voc:Grade a rdf:Property ;
    rdfs:label "grade"@en ;
    rdfs:comment "The grade achieved by the student in a course."@en ;
    rdfs:range rdfs:Literal .
    
voc:Competency a rdf:Property ;
    rdfs:label "competency"@en ;
    rdfs:comment "A topic in which the student is competent."@en ;
    rdfs:domain voc:Student ;
    rdfs:range rdfs:Literal .

#foaf:givenName 
#foaf:familyName 
#foaf:mbox 

voc:OfferedBy a rdf:Property ;
    rdfs:label "offered by"@en ;
    rdfs:comment "The university offering the course."@en ;
    rdfs:domain voc:Course ;
    rdfs:range voc:University .

voc:BelongsTo rdf:type rdf:Property ;
    rdfs:label "belongs to"@en ;
    rdfs:comment "The course that the lecture belongs to."@en ;
    rdfs:domain voc:Lecture ;
    rdfs:range voc:Course .

voc:ResourceName rdf:type rdf:Property 
    rdfs:label "resource name"@en ;
    rdfs:comment "The name of the resource material."@en ;
    rdfs:range voc:Literal .

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX schema: <http://schema.org/>
PREFIX dbpedia: <http://dbpedia.org/ontology/>
PREFIX wikidata: <https://www.wikidata.org/wiki/Property:>

SELECT ?courseName ?courseLabel
WHERE {
  ?university rdf:type :University ;
              :UniversityName "[university name]" ;
              :UniversityLink ?course .
  ?course rdf:type :Course ;
          :CourseName ?courseName ;
          rdfs:label ?courseLabel .
}

SELECT ?courseName ?courseLabel
WHERE {
  ?course rdf:type :Course ;
          :CourseName ?courseName .
  ?topic rdf:type :Topic ;
         :TopicName "[topic]" ;
         :TopicLink ?course .
  ?course rdfs:label ?courseLabel .
}

SELECT ?topicName ?topicLabel
WHERE {
  ?course rdf:type :Course ;
          :CourseName "[course]" .
  ?lecture rdf:type :Lecture ;
           :LectureNumber "[lecture number]" ;
           :LectureContent ?topic ;
           :CourseLink ?course .
  ?topic rdf:type :Topic ;
         :TopicName ?topicName .
  ?topic rdfs:label ?topicLabel .
}

SELECT ?courseName ?courseLabel
WHERE {
  ?university rdf:type :University ;
              :UniversityName "[university]" .
  ?course rdf:type :Course ;
          :CourseName ?courseName ;
          :UniversityLink ?university ;
          :CourseSubject "[subject]" .
  ?course rdfs:label ?courseLabel .
}

SELECT ?material ?materialLabel
WHERE {
  ?course rdf:type :Course ;
          :CourseName "[course]" ;
          :CourseNumber "[number]" .
  ?topic rdf:type :Topic ;
         :TopicName "[topic]" ;
         :TopicLink ?course .
  ?lecture rdf:type :Lecture ;
           :LectureContent ?material ;
           :CourseLink ?course .
  ?material rdfs:label ?materialLabel .
}

SELECT ?credits
WHERE {
  ?course rdf:type :Course ;
          :CourseName "[course]" ;
          :CourseNumber "[number]" ;
          :CourseCredits ?credits .
}

SELECT ?additionalResource ?resourceLabel
WHERE {
  ?course rdf:type :Course ;
          :CourseName "[course]" ;
          :CourseNumber "[number]" ;
          :CourseWebsite ?additionalResource .
  ?additionalResource rdfs:label ?resourceLabel .
}

SELECT ?lectureContent ?contentLabel
WHERE {
  ?course rdf:type :Course ;
          :CourseName "[course]" ;
          :CourseNumber "[number]" .
  ?lecture rdf:type :Lecture ;
           :LectureContent ?lectureContent ;
           :LectureNumber "[lecture number]" .
  ?lectureContent rdfs:label ?contentLabel .
}

SELECT ?readingMaterial ?materialLabel
WHERE {
  ?course rdf:type :Course ;
          :CourseName "[course]" .
  ?topic rdf:type :Topic ;
         :TopicName "[topic]" ;
         :TopicLink ?course .
  ?lecture rdf:type :Lecture ;
           :LectureContent ?readingMaterial ;
           :LectureWebsite ?course .
  ?readingMaterial rdfs:label ?materialLabel .
}

SELECT ?competency ?topicLabel
WHERE {
  ?course rdf:type :Course ;
          :CourseName "[course]" ;
          :CourseNumber "[number]" .
  ?student rdf:type :Student ;
           :CompletedCourse ?course ;
           :Competency ?competency .
  ?competency rdfs:label ?topicLabel .
}

SELECT ?grade
WHERE {
  ?student rdf:type :Student ;
           :FirstName "[student's first name]" ;
           :LastName "[student's last name]" ;
           :CompletedCourse [rdfs:label "[course]"] ;
           :Grade ?grade .
}

SELECT ?student ?studentLabel
WHERE {
  ?student rdf:type :Student ;
           :CompletedCourse [rdfs:label "[course]"] .
  ?student rdfs:label ?studentLabel .
}

SELECT ?courseName ?courseLabel ?grade
WHERE {
  ?student rdf:type :Student ;
           :FirstName "[student's first name]" ;
           :LastName "[student's last name]" ;
           :CompletedCourse ?course ;
           :Grade ?grade .
  ?course rdf:type :Course ;
          :CourseName ?courseName .
  ?course rdfs:label ?courseLabel .
}
